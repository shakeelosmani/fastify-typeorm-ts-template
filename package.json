{
  "name": "fastify-typeorm-ts-template",
  "version": "1.0.0",
  "description": "A starter template to setup a fastify typeorm ts project. This only contains dependencies.",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "concurrently --kill-others \"tsc -w\" \"nodemon dist/index.js\""
  },
  "keywords": [
    "Fastifty",
    "TypeOrm",
    "TypeScript"
  ],
  "author": "Shakeel Osmani",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.14.6",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "fastify": "^3.7.0",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.29"
  }
}
